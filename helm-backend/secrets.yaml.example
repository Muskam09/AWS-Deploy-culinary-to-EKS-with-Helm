# THIS IS AN EXAMPLE FILE.
# To run the project, create a copy of this file named 'secrets.yaml'
# and fill it with your actual credentials.
# The 'secrets.yaml' file is already included in .gitignore and will not be committed.

secret:
  # The full connection string for the PostgreSQL database.
  # Replace user, password, db-service, and dbname with your actual values.
  DATABASE_URL: "postgresql://user:password@db-service:5432/dbname" # <-- REPLACE WITH YOUR REAL DATA

  # The secret key used by Django for cryptographic signing.
  # You can generate a new one here: https://djecrety.ir/
  SECRET_KEY: "your-super-secret-django-key-goes-here" # <-- REPLACE WITH YOUR KEY